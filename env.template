# Supabase Configuration
# Get these from your Supabase project settings
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# N8n Configuration
# Update with your n8n instance details
VITE_N8N_BASE_URL=http://localhost:5678
VITE_N8N_API_KEY=your_n8n_api_key_here
VITE_N8N_WEBHOOK_SECRET=your_webhook_secret_here

# N8n Authentication (for debate webhook)
VITE_N8N_USERNAME=debate_user
VITE_N8N_PASSWORD=debate_pass

# Optional: Google OAuth (if using Google services in n8n)
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret

# Optional: OpenAI (if using AI features)
VITE_OPENAI_API_KEY=your_openai_api_key

# Optional: ElevenLabs (for TTS features)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key
